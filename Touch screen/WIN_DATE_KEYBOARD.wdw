            	// Finds the row containing the stored value
            	nRow = LooperSeek(ATT_STORED_VAL, vValue, True)		
            ELSE
            	// Direct row
            	nRow = vValue
            END
            
            // If the row is valid
            IF nRow > 0 _AND_ nRow <= LOOP_CONTENT..Occurrence THEN
            	
            	// Move on the row
            	__ScrollTo(MyselfName, nRow, 1, False, False, *, True)		
            END
